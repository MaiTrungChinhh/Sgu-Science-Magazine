{
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "punctuation.section.embedded.begin.smarty",
          "punctuation.section.embedded.end.smarty"
        ],
        "settings": {
          "foreground": "#FFA500",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "keyword.control.smarty",
          "support.function.built-in.smarty"
        ],
        "settings": {
          "foreground": "#16A016",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "variable.parameter.smarty"
        ],
        "settings": {
          "foreground": "#AE23A3",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "source.smarty"
        ],
        "settings": {
          "foreground": "#D17C32"
        }
      }
    ]
  }
}